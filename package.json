{
  "name": "etark",
  "version": "1.0.0",
  "description": "Believe in argument",
  "main": "index.js",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "tslint": "^6.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.23",
    "@types/sequelize": "4.28.9",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "moment": "^2.27.0",
    "mysql2": "^2.1.0",
    "nodemon": "^2.0.4",
    "sequelize": "6.3.3",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "start": "node -r tsconfig-paths/register -r ts-node/register ./src/app.ts",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bornprogrammer/etark-node.git"
  },
