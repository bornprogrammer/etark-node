  "description": "Believe in argument",
  "main": "index.js",
  "dependencies": {
    "@types/validator": "^13.1.0",
    "body-parser": "^1.19.0",
    "checksum": "^0.1.1",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "html-pdf": "^2.2.0",
    "joi": "^17.2.1",
    "moment": "^2.27.0",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.11",
    "paytmchecksum": "^1.5.0",
    "promise-request-retry": "^1.0.2",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "sequelize": "6.3.3",
    "tedious": "^9.2.1",
    "ts-jest": "^26.2.0",
    "tslint": "^6.1.3",
    "tspath": "^1.3.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/sequelize": "4.28.9",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "start": "node -r tsconfig-paths/register -r ts-node/register -r dotenv/config ./src/app.ts",
    "start:watch": "nodemon",
    "test": "jest --watch --maxWorkers=1",
    "build": "tsc",
    "prod": "cd dist/src && node -r tsconfig-paths/register -r dotenv/config app.js",
    "pm2": "pm2 start 'npm run prod' --name etark-node"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bornprogrammer/etark-node.git"
  },
